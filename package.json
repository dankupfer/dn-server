{
  "name": "@dankupfer/dn-server",
  "version": "1.0.0",
  "description": "Express server for DN project services - Figma bridge, customer generation, AI assist, and Gemini Live API proxy",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "type-check": "tsc --noEmit"
