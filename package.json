{
  "name": "@dankupfer/dn-server",
  "version": "1.0.0",
  "description": "Express server for DN project services - Figma bridge, customer generation, AI assist, and Gemini Live API proxy",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:ui": "esbuild src/views/figma/scripts/main.ts --bundle --outfile=src/views/figma/compiled/bundle.js --format=iife",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "type-check": "tsc --noEmit",
    "deploy": "./deploy.sh"
  },
  "keywords": [
    "dn",
    "figma",
    "bridge",
    "gemini",
    "voice",
    "banking",
    "express"
  ],
  "author": "Dan Kupfer",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.3.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.22.0",
    "@google/generative-ai": "^0.24.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.1.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.5",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.19.15",
    "@types/ws": "^8.5.13",
    "esbuild": "^0.25.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
